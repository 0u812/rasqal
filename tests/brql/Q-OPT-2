SELECT *
WHERE 
	(?a rdf:type ns:Type1) 

OPTIONAL
 	// Yes - :y only
 	(?a ns:p2 ?b)
 	(?a ns:p3 ?bb)

OPTIONAL
	// Yes
 	(?a ns:q ?q)

OPTIONAL
	// Never
	(?a ns:zzz ?c)

USING 
	rdf   FOR <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
	ns    FOR <http://example.org/ns#>
	data  FOR <http://example.org/>

